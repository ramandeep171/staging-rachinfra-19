<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_picking_contract_issue" model="ir.sequence">
            <field name="name">Contract Issue Product Sequence</field>
            <field name="prefix">CIP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="ref('base.main_company')"/>
        </record>

        <record id="picking_type_contract_issue" model="stock.picking.type">
            <field name="name">Contract Issue Product</field>
            <field name="code">internal</field>
            <field name="sequence_code">CIP</field>
            <field name="sequence_id" ref="seq_picking_contract_issue"/>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="default_location_src_id" ref="stock.stock_location_stock"/>
            <field name="default_location_dest_id" ref="stock.stock_location_customers"/>
        </record>
    </data>
</odoo>
