<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="seq_subcontractor_code" model="ir.sequence">
        <field name="name">Subcontractor Code</field>
        <field name="code">rmc.subcontractor.code</field>
        <field name="padding">4</field>
        <field name="prefix">SUBC-%(subc_geo)s-%(y)s-</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_transport_code" model="ir.sequence">
        <field name="name">Transport Mixer Code</field>
        <field name="code">rmc.transport.code</field>
        <field name="padding">3</field>
        <field name="prefix">TMX-%(plant_code)s-</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_plant_code" model="ir.sequence">
        <field name="name">Subcontractor Plant Code</field>
        <field name="code">rmc.plant.code</field>
        <field name="padding">4</field>
        <field name="prefix">PLANT-</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_pump_code" model="ir.sequence">
        <field name="name">Pump Code</field>
        <field name="code">rmc.pump.code</field>
        <field name="padding">4</field>
        <field name="prefix">PUMP-</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
