    <record id="action_rmc_subcontractor_config_geo" model="ir.actions.act_window">
        <field name="name">Geo Areas</field>
        <field name="res_model">rmc.subcontractor.geo.area</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="action_rmc_subcontractor_config_hot_zone" model="ir.actions.act_window">
        <field name="name">Hot Zones</field>
        <field name="res_model">rmc.subcontractor.hot.zone</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="action_rmc_subcontractor_config_stage" model="ir.actions.act_window">
        <field name="name">Pipeline Stages</field>
        <field name="res_model">rmc.subcontractor.stage</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="action_rmc_subcontractor_onboard_wizard" model="ir.actions.act_window">
        <field name="name">Onboarding Wizard</field>
        <field name="res_model">rmc.subcontractor.onboard.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
