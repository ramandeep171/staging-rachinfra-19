<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="action_rmc_subcontractor_pipeline" model="ir.actions.act_window">
        <field name="name">Subcontractor Pipeline</field>
        <field name="res_model">rmc.subcontractor</field>
        <field name="view_mode">kanban,list,form</field>
        <field name="context">{}</field>
    </record>

    <record id="action_rmc_subcontractor_profiles" model="ir.actions.act_window">
        <field name="name">Subcontractor Profiles</field>
        <field name="res_model">rmc.subcontractor.profile</field>
        <field name="view_mode">list,form</field>
