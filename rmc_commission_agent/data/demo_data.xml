<odoo>
    <data noupdate="1">
        <record id="rmc_commission_master_dropship_ncr" model="rmc.commission.master">
            <field name="name">Dropshipping NCR Master</field>
            <field name="country_tag">ncr</field>
            <field name="applicable_channel">rmc_dropshipping</field>
        </record>
        <record id="rmc_commission_master_rental_haryana" model="rmc.commission.master">
            <field name="name">Rental Haryana Master</field>
            <field name="country_tag">haryana</field>
            <field name="applicable_channel">rental</field>
        </record>
        <record id="rmc_commission_master_distribution_ncr" model="rmc.commission.master">
            <field name="name">Distribution NCR Master</field>
            <field name="country_tag">ncr</field>
            <field name="applicable_channel">distribution</field>
        </record>
        <record id="rmc_commission_master_retention_haryana" model="rmc.commission.master">
            <field name="name">Retention Haryana Master</field>
            <field name="country_tag">haryana</field>
            <field name="applicable_channel">retention</field>
        </record>

        <record id="res_partner_rmc_agent_dropship" model="res.partner">
            <field name="name">RMC Agent Dropship</field>
            <field name="email">dropship.agent@example.com</field>
            <field name="company_type">company</field>
        </record>
        <record id="res_partner_rmc_agent_rental" model="res.partner">
            <field name="name">RMC Agent Rental</field>
            <field name="email">rental.agent@example.com</field>
            <field name="company_type">company</field>
        </record>
        <record id="res_partner_rmc_agent_distribution" model="res.partner">
            <field name="name">RMC Agent Distribution</field>
            <field name="email">distribution.agent@example.com</field>
            <field name="company_type">company</field>
        </record>
        <record id="res_partner_rmc_agent_retention" model="res.partner">
            <field name="name">RMC Agent Retention</field>
            <field name="email">retention.agent@example.com</field>
            <field name="company_type">company</field>
        </record>

        <record id="rmc_commission_agent_dropship_demo" model="rmc.commission.agent">
            <field name="name">Dropshipping Demo Agent</field>
            <field name="partner_id" ref="res_partner_rmc_agent_dropship"/>
            <field name="agent_type">rmc_dropshipping</field>
            <field name="country_tag">ncr</field>
            <field name="commission_master_id" ref="rmc_commission_master_dropship_ncr"/>
        </record>
        <record id="rmc_commission_agent_rental_demo" model="rmc.commission.agent">
            <field name="name">Rental Demo Agent</field>
            <field name="partner_id" ref="res_partner_rmc_agent_rental"/>
            <field name="agent_type">rental</field>
            <field name="country_tag">haryana</field>
            <field name="commission_master_id" ref="rmc_commission_master_rental_haryana"/>
        </record>
        <record id="rmc_commission_agent_distribution_demo" model="rmc.commission.agent">
            <field name="name">Distribution Demo Agent</field>
            <field name="partner_id" ref="res_partner_rmc_agent_distribution"/>
            <field name="agent_type">distribution</field>
            <field name="country_tag">ncr</field>
            <field name="commission_master_id" ref="rmc_commission_master_distribution_ncr"/>
        </record>
        <record id="rmc_commission_agent_retention_demo" model="rmc.commission.agent">
            <field name="name">Retention Demo Agent</field>
            <field name="partner_id" ref="res_partner_rmc_agent_retention"/>
            <field name="agent_type">retention</field>
            <field name="country_tag">haryana</field>
            <field name="commission_master_id" ref="rmc_commission_master_retention_haryana"/>
        </record>
    </data>
</odoo>
