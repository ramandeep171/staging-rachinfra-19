<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="action_report_daily_mo" model="ir.actions.report">
      <field name="name">RMC Daily MO Report</field>
      <field name="model">mrp.production</field>
      <field name="report_name">gear_on_rent.report_daily_mo</field>
      <field name="report_type">qweb-pdf</field>
      <field name="print_report_name">'%s - Daily MO' % (object.name or 'MO')</field>
      <field name="binding_model_id" ref="mrp.model_mrp_production"/>
    </record>

    <template id="report_daily_mo">
      <t t-call="web.html_container">
        <t t-foreach="docs" t-as="doc">
          <t t-set="payload" t-value="doc._gear_get_daily_report_payload()"/>
          <t t-call="web.external_layout">
            <div class="gor-page">
            <style>
              .gor-page{width:90%;margin:0 auto;}
              .gor-kv-table{width:100%;border-collapse:collapse;margin-bottom:12px;}
              .gor-kv-table th{padding:2px 6px;text-align:left;font-weight:600;border:none;}
              .gor-kv-table td{padding:2px 6px;text-align:right;border:none;}
              .gor-section-title{margin-top:18px;}
              .gor-metric-table{width:100%;border-collapse:collapse;margin-bottom:14px;font-size:12px;border:none !important;border-radius:0 !important;}
              .gor-metric-table th{padding:4px 6px;text-align:left;border:none !important;border-bottom:1px solid #bbb !important;}
              .gor-metric-table td{padding:4px 6px;text-align:right;border:none !important;border-bottom:1px solid #ddd !important;}
              .gor-metric-table tr:last-child th,
              .gor-metric-table tr:last-child td{border-bottom:none !important;}
              .gor-metric-table.gor-metric-table--left td{text-align:left;}
              .gor-metric-table.gor-metric-table--plain th,
              .gor-metric-table.gor-metric-table--plain td{border:none;}
              .gor-two-col{width:100%;margin-top:16px;display:flex;flex-wrap:wrap;gap:24px 0;}
              .gor-two-col--metrics{margin-top:0;}
              .gor-two-col__col{flex:1 1 50%;padding:0 12px 0 0;}
              .gor-two-col__col:last-child{padding-left:12px;padding-right:0;}
              .gor-two-col--metrics{margin-top:0;}
              @page{
                margin:20mm 15mm 25mm 15mm;
                @bottom-right{
                  content:element(gor-footer);
                }
              }
              .gor-inline-table{width:100%;}
              .gor-inline-table th,
              .gor-inline-table td{border:none;border-bottom:1px solid #ddd;}
              .gor-inline-table tr:last-child th,
              .gor-inline-table tr:last-child td{border-bottom:none;}
              .gor-full-table{width:100%;border-collapse:collapse;margin-top:16px;font-size:12px;}
              .gor-full-table th,
              .gor-full-table td{padding:4px 6px;border:none;border-bottom:1px solid #ddd;}
              .gor-full-table th{text-align:left;border-bottom:1px solid #bbb;}
              .gor-full-table td{text-align:right;}
              .gor-full-table tbody tr:last-child td{border-bottom:none;}
              .gor-full-table thead{display:table-header-group;}
              .gor-text-left{text-align:left;}
              .gor-text-center{text-align:center;}
              .gor-footer{position:running(gor-footer);font-size:10px;text-align:right;}
              .gor-footer-placeholder{height:12mm;}
            </style>

            <div class="row mb-3">
              <div class="col-7">
                <h2>Daily Manufacturing Order Summary</h2>
                <div style="font-size:12px; line-height:1.7;">
                  <div><strong>Manufacturing Order:</strong>
                    <span t-esc="payload.get('invoice_name')"/></div>
                  <div><strong>Contract:</strong>
                    <span t-esc="payload.get('contract_name')"/></div>
                  <div><strong>Customer:</strong>
                    <span t-esc="payload.get('customer_name')"/></div>
                </div>
              </div>
              <div class="col-5 text-end" style="text-align: right;">
                <p class="h4 mb-1" t-esc="payload.get('month_label')"/>
                <p class="lead mb-0">
                  <strong t-esc="payload.get('version_label')"/>
                </p>
              </div>
            </div>
            <t t-set="prime_val" t-value="payload.get('prime_output_qty', 0.0)"/>
            <t t-set="ngt_qty_val" t-value="payload.get('ngt_qty', 0.0)"/>
            <!-- Recompute standby using monthly MGQ (target) first, then daily/adjusted fallbacks -->
            <t t-set="target_val" t-value="payload.get('monthly_target_qty', 0.0) or payload.get('target_qty', 0.0) or payload.get('adjusted_target_qty', 0.0) or payload.get('monthly_adjusted_qty', 0.0)"/>
            <t t-set="standby_val" t-value="max(target_val - (prime_val + ngt_qty_val), 0.0)"/>
            <t t-set="total_invoice_qty" t-value="prime_val + standby_val + ngt_qty_val"/>
            <t t-set="net_effort_hours" t-value="payload.get('ngt_hours', 0.0)"/>

            <div style="margin:12px 0;border:1px solid #0b5394;border-radius:8px;padding:10px;background:#f6f8fc;">
              <div style="font-weight:700;font-size:14px;margin-bottom:6px;color:#0b5394;">
                PRODUCTION OVERVIEW - BASIS INFORMATION (<t t-esc="payload.get('month_label')"/>)
              </div>
              <table class="gor-full-table" style="font-size:11px;">
                <thead style="display: table-header-group;background:#9c3e0f;color:#fff;">
                  <tr>
                    <th style="width:6%;">Sr. No.</th>
                    <th style="width:32%;">Variables</th>
                    <th style="width:22%;">Calculation</th>
                    <th style="width:20%;">Quantity (m³) / Value</th>
                    <th style="width:20%;">Remarks</th>
                  </tr>
                </thead>
                <tbody>
                  <tr><td>1</td><td class="gor-text-left">Inventory Mode</td><td>Input</td><td class="gor-text-left"><t t-esc="payload.get('inventory_mode') or 'N/A'"/></td><td class="gor-text-left">-</td></tr>
                  <tr><td>2</td><td class="gor-text-left">Real Warehouse</td><td>Input</td><td class="gor-text-left"><t t-esc="payload.get('real_warehouse') or 'N/A'"/></td><td class="gor-text-left">-</td></tr>
                  <tr><td>3</td><td class="gor-text-left">Net Effort Window</td><td>Time Delta Assessment</td><td><t t-esc="net_effort_hours"/></td><td class="gor-text-left">Active production window</td></tr>
                  <tr><td>4</td><td class="gor-text-left">NGT Hours</td><td>Captured</td><td><t t-esc="net_effort_hours"/></td><td class="gor-text-left">-</td></tr>
                  <tr><td>5</td><td class="gor-text-left">Attendance Present</td><td>Count</td><td class="gor-text-left"><t t-esc="payload.get('attendance_present', 0)"/></td><td class="gor-text-left">-</td></tr>
                  <tr><td>6</td><td class="gor-text-left">Attendance Total Hours</td><td>Hours</td><td class="gor-text-left"><t t-esc="('%.2f' % (payload.get('attendance_hours_total', 0.0) or 0.0)).rstrip('0').rstrip('.')"/></td><td class="gor-text-left">-</td></tr>
                  <tr><td>7</td><td class="gor-text-left">AbhiTak MGQ Target</td><td>Input</td><td><t t-esc="payload.get('monthly_target_qty', 0.0)"/></td><td class="gor-text-left">Final MGQ Target</td></tr>
                  <tr><td>8</td><td class="gor-text-left">AbhiTak Adjusted MGQ</td><td>Adjustment</td><td><t t-esc="payload.get('monthly_adjusted_qty', 0.0)"/></td><td class="gor-text-left">-</td></tr>
                  <tr><td>9</td><td class="gor-text-left"><strong>AbhiTak Prime Output (m³)</strong></td><td><strong>As per production log</strong></td><td><strong><t t-esc="prime_val"/></strong></td><td class="gor-text-left">-</td></tr>
                  <tr><td>10</td><td class="gor-text-left">AbhiTak Optimized Standby (m³)</td><td>MGQ - (Prime Output + NGT)</td><td><t t-esc="standby_val"/></td><td class="gor-text-left">Standby balancing</td></tr>
                  <tr><td>11</td><td class="gor-text-left">AbhiTak NGT Relief (m³)</td><td>NGT Hours × Rate</td><td><t t-esc="ngt_qty_val"/></td><td class="gor-text-left">Billing quantity</td></tr>
                  <tr><td>12</td><td class="gor-text-left">Total Invoice Quantity</td><td>Prime + NGT + Standby</td><td><t t-esc="total_invoice_qty"/></td><td class="gor-text-left">Billing quantity</td></tr>
                </tbody>
              </table>
              <div style="margin-top:6px;font-size:10px;">
                <div><strong>Materials Shortage:</strong> <t t-esc="payload.get('materials_shortage') or 'N/A'"/></div>
                <div><strong>Manpower:</strong> <t t-esc="payload.get('manpower_notes') or 'N/A'"/></div>
                <div><strong>Assets:</strong> <t t-esc="payload.get('asset_notes') or 'N/A'"/></div>
              </div>
            </div>

            <t t-if="payload.get('manual_operations')">
              <div style="margin:12px 0 14px 0;border:1px solid #0b5394;border-radius:8px;padding:10px;background:#f6f8fc;">
                <div style="font-weight:700;font-size:14px;margin-bottom:6px;color:#0b5394;">PRIME MANUAL OUTPUT BREAKDOWN</div>
                <t t-set="manual_ops" t-value="payload.get('manual_operations') or []"/>
                <t t-set="manual_total" t-value="payload.get('manual_total_qty', 0.0)"/>
                <t t-set="prime_base" t-value="payload.get('prime_output_qty', 0.0) - manual_total"/>
                <t t-set="prime_combined" t-value="payload.get('prime_output_qty', 0.0)"/>
                <table class="gor-full-table" style="font-size:11px;">
                  <thead style="background:#9c3e0f;color:#fff;display: table-header-group;">
                    <tr>
                      <th style="width:8%;">Sr.</th>
                      <th style="width:50%;">Description</th>
                      <th style="width:22%;">Quantity (m³)</th>
                      <th style="width:20%;">Remarks</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td>1</td>
                      <td class="gor-text-left">Prime Output (system)</td>
                      <td><t t-esc="prime_base"/></td>
                      <td class="gor-text-left">Base production</td>
                    </tr>
                    <t t-foreach="[(i + 2, op) for i, op in enumerate(manual_ops)]" t-as="row">
                      <tr>
                        <td><t t-esc="row[0]"/></td>
                        <td class="gor-text-left"><t t-esc="row[1].get('description')"/></td>
                        <td><t t-esc="row[1].get('quantity', 0.0)"/></td>
                        <td class="gor-text-left"><t t-esc="row[1].get('remarks')"/></td>
                      </tr>
                    </t>
                  </tbody>
                  <tfoot>
                    <tr>
                      <th colspan="2" class="gor-text-right">TOTAL PRIME MANUAL</th>
                      <th><t t-esc="manual_total"/></th>
                      <th></th>
                    </tr>
                    <tr>
                      <th colspan="2" class="gor-text-right">TOTAL PRIME OUTPUT (incl. manual)</th>
                      <th><t t-esc="prime_combined"/></th>
                      <th></th>
                    </tr>
                  </tfoot>
                </table>
              </div>
            </t>

            <t t-if="payload.get('scrap_logs')">
              <div style="margin:10px 0;border:1px solid #0b5394;border-radius:6px;padding:10px;background:#f6f8fc;">
                <div style="font-weight:700;font-size:14px;margin-bottom:6px;color:#0b5394;">WASTAGE &amp; SCRAP ANNEXURE</div>
                <t t-set="scrap_refs" t-value="', '.join([log.get('reference') for log in payload.get('scrap_logs') if log.get('reference')])"/>
                <div style="font-size:11px;margin-bottom:8px;">
                  <strong>References:</strong> <t t-esc="scrap_refs or '-'"/> |
                  <strong>Contract / SO:</strong> <t t-esc="payload.get('contract_name') or '-'"/> |
                  <strong>Status:</strong> <t t-esc="payload.get('status_label') or 'Approved'"/> |
                  <strong>MGQ (Monthly):</strong> <t t-esc="payload.get('monthly_target_qty', 0.0)"/>
                </div>
                <t t-set="scrap_total" t-value="sum(log.get('quantity', 0.0) for log in payload.get('scrap_logs'))"/>
                <table class="gor-full-table" style="margin-bottom:10px;font-size:11px;">
                  <thead style="display: table-header-group;background:#9c3e0f;color:#fff;">
                    <tr>
                      <th style="width:6%;">Sr.</th>
                      <th class="gor-text-left" style="width:22%;">Date</th>
                      <th class="gor-text-left" style="width:28%;">Reference</th>
                      <th class="gor-text-left" style="width:24%;">Reason</th>
                      <th style="width:20%;">Scrap Qty (m³)</th>
                    </tr>
                  </thead>
                  <tbody>
                    <t t-foreach="[(i + 1, log) for i, log in enumerate(payload.get('scrap_logs'))]" t-as="row">
                      <tr>
                        <td><t t-esc="row[0]"/></td>
                        <td class="gor-text-left"><t t-esc="row[1].get('date') or 'N/A'"/></td>
                        <td class="gor-text-left"><t t-esc="row[1].get('reference') or 'N/A'"/></td>
                        <td class="gor-text-left"><t t-esc="row[1].get('reason') or '-'"/></td>
                        <td><t t-esc="row[1].get('quantity', 0.0)"/></td>
                      </tr>
                    </t>
                  </tbody>
                  <tfoot>
                    <tr>
                      <th colspan="4" class="gor-text-right">TOTAL SCRAP (m³)</th>
                      <th><t t-esc="scrap_total"/></th>
                    </tr>
                  </tfoot>
                </table>

                <table class="gor-full-table" style="font-size:11px;">
                  <thead style="background:#9c3e0f;color:#fff;display: table-header-group;">
                    <tr>
                      <th style="width:8%;">Sr. No.</th>
                      <th style="width:52%;">Variables</th>
                      <th style="width:20%;">Value</th>
                      <th style="width:20%;">Remarks</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr><td>1</td><td class="gor-text-left">Prime Output (m³)</td><td><t t-esc="payload.get('prime_output_qty', 0.0)"/></td><td>-</td></tr>
                    <tr><td>2</td><td class="gor-text-left">Allowed Wastage (m³)</td><td><t t-esc="payload.get('allowed_wastage_qty', 0.0)"/></td><td>-</td></tr>
                    <tr><td>3</td><td class="gor-text-left">Actual Scrap (m³)</td><td><t t-esc="payload.get('actual_scrap_qty', 0.0)"/></td><td>-</td></tr>
                    <tr><td>4</td><td class="gor-text-left">Over Wastage / Deduction (m³)</td><td><t t-esc="payload.get('over_wastage_qty', 0.0)"/></td><td>-</td></tr>
                  </tbody>
                </table>
              </div>
            </t>

            <t t-if="payload.get('loto_requests')">
              <div style="page-break-inside:avoid;margin-top:12px;border:1px solid #0b5394;border-radius:8px;padding:12px;">
                <t t-set="loto_list" t-value="payload.get('loto_requests') or []"/>
                <t t-set="loto_total_hours" t-value="payload.get('loto_total_hours', 0.0) or sum(l.get('hours_total', 0.0) for l in loto_list)"/>
                <t t-set="loto_chargeable_hours" t-value="payload.get('loto_chargeable_hours', 0.0)"/>
                <h4 style="margin:0 0 6px 0;color:#0b5394;">LOTO / Breakdown Annexure</h4>
                <div style="display:flex;flex-wrap:wrap;gap:8px 20px;font-size:11px;margin-bottom:6px;">
                  <div><strong>References:</strong> <t t-esc="', '.join(l.get('name') or '' for l in loto_list)"/></div>
                  <div><strong>Contract / SO:</strong> <t t-esc="payload.get('contract_name') or ''"/></div>
                  <div><strong>Status:</strong> <t t-esc="payload.get('status_label') or 'Approved'"/></div>
                  <div><strong>MGQ (Monthly):</strong> <t t-esc="payload.get('monthly_target_qty', 0.0)"/></div>
                </div>
                <table class="gor-full-table" style="margin-bottom:10px;">
                  <thead style="display: table-header-group;">
                    <tr>
                      <th>Reference</th>
                      <th>Period</th>
                      <th>Total Hours</th>
                      <th>Wave-Off Applied</th>
                      <th>Chargeable</th>
                      <th>Reason</th>
                    </tr>
                  </thead>
                  <tbody>
                    <t t-foreach="loto_list" t-as="loto">
                      <tr>
                        <td class="gor-text-left"><t t-esc="loto.get('name')"/></td>
                        <td class="gor-text-left">
                          <div><t t-esc="loto.get('period_start')"/></div>
                          <div><t t-esc="loto.get('period_end')"/></div>
                        </td>
                        <td><t t-esc="loto.get('hours_total', 0.0)"/></td>
                        <td><t t-esc="loto.get('hours_waveoff', 0.0)"/></td>
                        <td><t t-esc="loto.get('hours_chargeable', 0.0)"/></td>
                        <td class="gor-text-left"><t t-esc="loto.get('reason')"/></td>
                      </tr>
                    </t>
                  </tbody>
                </table>
                <table class="gor-full-table" style="font-size:11px;">
                  <thead style="display: table-header-group;">
                    <tr>
                      <th class="gor-text-left">Label</th>
                      <th>Value</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <th class="gor-text-left">Total LOTO Approved Hours</th>
                      <td><t t-esc="loto_total_hours"/></td>
                    </tr>
                    <tr>
                      <th class="gor-text-left">Wave-Off Allowance</th>
                      <td><t t-esc="payload.get('waveoff_allowance', 0.0)"/></td>
                    </tr>
                    <tr>
                      <th class="gor-text-left">Wave-Off Applied This Month</th>
                      <td style="color:#198754;"><t t-esc="payload.get('waveoff_applied', 0.0)"/></td>
                    </tr>
                    <tr>
                      <th class="gor-text-left">Chargeable LOTO Hours</th>
                      <td style="color:#ba1b1b;"><t t-esc="loto_chargeable_hours"/></td>
                    </tr>
                  </tbody>
                </table>

                <t t-set="loto_period_starts" t-value="', '.join([l.get('period_start') or '' for l in loto_list])"/>
                <t t-set="loto_period_ends" t-value="', '.join([l.get('period_end') or '' for l in loto_list])"/>
                <div style="margin:8px 0 0 0;padding:8px;border-left:4px solid #0b5394;background:#f3f7fb;font-size:11px;">
                  <div style="font-weight:600;margin-bottom:4px;">LOTO Period Calculation Details</div>
                  <div>LOTO Period Hours: <t t-esc="loto_total_hours"/></div>
                  <div>Time delta window: Start <t t-esc="loto_period_starts"/> | End <t t-esc="loto_period_ends"/></div>
                  <div>Chargeable LOTO Hours: <t t-esc="loto_chargeable_hours"/></div>
                  <div>Total Duration (days): <t t-esc="('%.2f' % (loto_chargeable_hours / 24.0)) if loto_chargeable_hours else '0.00'"/></div>
                </div>
              </div>
            </t>

            <h4 class="mt-4">Manufacturing Orders</h4>
            <table class="gor-full-table">
              <thead style="display: table-header-group;">
                <tr>
                  <th>Date</th>
                  <th>Reference</th>
                  <th>Cooling</th>
                  <th>Daily MGQ</th>
                  <th>Adjusted MGQ</th>
                  <th>Prime Output</th>
                  <th>Allowed Wastage</th>
                  <th>Actual Scrap</th>
                  <th>Over Wastage</th>
                  <th>Deduction</th>
                  <th>Optimized Standby</th>
                  <th>NGT Hours</th>
                  <th>LOTO Hours</th>
                </tr>
              </thead>
              <tbody>
                <tr t-if="not payload.get('manufacturing_orders')">
                  <td colspan="9" class="gor-text-center">
                    No manufacturing orders were recorded for this day.
                  </td>
                </tr>
                <t t-foreach="payload.get('manufacturing_orders', [])" t-as="mo">
                  <tr>
                    <td class="gor-text-left">
                      <t t-esc="mo.get('date_start')"/>
                    </td>
                    <td class="gor-text-left">
                      <t t-esc="mo.get('reference')"/>
                    </td>
                    <td class="gor-text-center">
                      <span t-esc="mo.get('is_cooling') and 'Yes' or 'N/A'"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('daily_mgq', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('adjusted_mgq', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('prime_output', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('allowed_wastage', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('actual_scrap', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('over_wastage', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('deduction', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('optimized_standby', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('ngt_hours', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="mo.get('loto_hours', 0.0)"/>
                    </td>
                  </tr>
                </t>
              </tbody>
            </table>

            <t t-if="payload.get('include_attendance_annexure') and payload.get('attendance_entries')">
              <div style="margin:14px 0;border:1px solid #0b5394;border-radius:10px;overflow:hidden;background:#fff;">
                <div style="background:#fff;color:#0b5394;padding:8px 12px;font-weight:700;font-size:13px;text-transform:uppercase;letter-spacing:0.3px;border-bottom:1px solid #0b5394;">
                  Attendance Annexure
                </div>
                <table class="gor-full-table" style="margin:0;font-size:11px;background:#fff;">
                  <thead style="display: table-header-group;background:#9c3e0f;color:#fff;">
                    <tr>
                      <th style="width:6%;">Sr.</th>
                      <th class="gor-text-left" style="width:34%;">Employee</th>
                      <th class="gor-text-left" style="width:18%;">Check In</th>
                      <th class="gor-text-left" style="width:18%;">Check Out</th>
                      <th style="width:12%;">Worked Hours</th>
                      <th class="gor-text-left" style="width:12%;">Status</th>
                    </tr>
                  </thead>
                  <tbody>
                    <t t-set="total_hours" t-value="0.0"/>
                    <t t-foreach="payload.get('attendance_entries', [])" t-as="att" t-foreach-index="idx">
                      <t t-set="total_hours" t-value="total_hours + (att.get('worked_hours', 0.0) or 0.0)"/>
                      <tr>
                        <td><t t-esc="(idx or 0) + 1"/></td>
                        <td class="gor-text-left"><t t-esc="att.get('employee_name')"/></td>
                        <td class="gor-text-left"><t t-esc="att.get('check_in')"/></td>
                        <td class="gor-text-left"><t t-esc="att.get('check_out') or '-'"/></td>
                        <td><t t-esc="att.get('worked_hours', 0.0)"/></td>
                        <td class="gor-text-left"><t t-esc="att.get('status')"/></td>
                      </tr>
                    </t>
                    <tr style="font-weight:700;background:#eef3fb;border-top:1px solid #c9d6ed;">
                      <td colspan="6" class="gor-text-left">
                        Present: <t t-esc="payload.get('attendance_present', 0)"/> |
                        Total Hours: <t t-esc="('%.2f' % total_hours).rstrip('0').rstrip('.')"/>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </t>
            <h4 class="mt-4">Production Dockets</h4>
            <table class="gor-full-table">
              <thead style="display: table-header-group;">
                <tr>
                  <th>Docket</th>
                  <th>Batching Time</th>
                  <th>Start Time</th>
                  <th>End Time</th>
                  <th>Duration (min)</th>
                  <th>Customer</th>
                  <th>Customer Address</th>
                  <th>Quantity (m³)</th>
                  <th>Manual On Production</th>
                  <th>Manual After Production</th>
                  <th>TM Number</th>
                  <th>Recipe</th>
                </tr>
              </thead>
              <tbody>
                <tr t-if="not payload.get('dockets')">
                  <td colspan="6" class="gor-text-center">
                    No production dockets were recorded for this day.
                  </td>
                </tr>
                <t t-foreach="payload.get('dockets', [])" t-as="docket">
                  <tr>
                    <td class="gor-text-left">
                      <t t-esc="docket.get('docket_no')"/>
                    </td>
                    <td class="gor-text-left">
                      <t t-esc="docket.get('batching_time')"/>
                    </td>
                    <td class="gor-text-left">
                      <t t-esc="docket.get('start_time') or 'N/A'"/>
                    </td>
                    <td class="gor-text-left">
                      <t t-esc="docket.get('end_time') or 'N/A'"/>
                    </td>
                    <td>
                      <t t-esc="docket.get('duration_minutes', 0.0)"/>
                    </td>
                    <td class="gor-text-left">
                      <t t-esc="docket.get('customer_name')"/>
                    </td>
                    <td class="gor-text-left">
                      <t t-esc="docket.get('customer_address')"/>
                    </td>
                    <td>
                      <t t-esc="docket.get('quantity_ordered', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="docket.get('manual_on_qty', 0.0)"/>
                    </td>
                    <td>
                      <t t-esc="docket.get('manual_after_qty', 0.0)"/>
                    </td>
                    <td class="gor-text-left">
                      <t t-esc="docket.get('tm_number')"/>
                    </td>
                    <td class="gor-text-left">
                      <t t-esc="docket.get('recipe')"/>
                    </td>
                  </tr>
                </t>
              </tbody>
            </table>

            <div class="gor-footer-placeholder"/>
            <div class="gor-footer">Daily MO Report</div>
            </div>
          </t>
        </t>
      </t>
    </template>
  </data>
</odoo>
